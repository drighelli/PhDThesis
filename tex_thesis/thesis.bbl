  \verb 10.1093/nar/gkt214
  \endverb
  \field{isbn}{1362-4962 (Electronic)$\backslash$n0305-1048 (Linking)}
  \field{issn}{03051048}
  \field{title}{{The Subread aligner: Fast, accurate and scalable read mapping
  by seed-and-vote}}
