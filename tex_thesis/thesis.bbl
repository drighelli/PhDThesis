  \field{title}{{ChIP-seq: Advantages and challenges of a maturing technology}}
  \field{volume}{10}
  \field{journaltitle}{Nature Reviews Genetics}
  \field{eprinttype}{arXiv}
  \field{year}{2009}
\endentry

