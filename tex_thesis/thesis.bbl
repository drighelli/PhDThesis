  \field{issn}{13674803}
  \field{title}{{Fast and accurate long-read alignment with Burrows-Wheeler
  transform}}
  \field{journaltitle}{Bioinformatics}
  \field{eprinttype}{arXiv}
  \field{year}{2010}
\endentry

