  \field{issn}{10889051}
  \field{number}{7}
  \field{pages}{1118\bibrangedash 1129}
  \field{title}{{DNase-seq predicts regions of rotational nucleosome stability
  across diverse human cell types}}
  \field{volume}{23}
  \field{journaltitle}{Genome Research}
  \field{year}{2013}
\endentry

