journal = {BMC bioinformatics},
keywords = {Base Composition,Gene Expression Regulation,High-Throughput Nucleotide Sequencing,High-Throughput Nucleotide Sequencing: methods,Humans,RNA,RNA: analysis,RNA: genetics,Sequence Alignment,Sequence Analysis, RNA,Sequence Analysis, RNA: methods,bias,ngs,rna,rna-seq},
mendeley-tags = {bias,ngs,rna,rna-seq},
number = {Suppl 1},
