keywords = {Gene Regulatory Networks,Humans,Insulin,Insulin: metabolism,Notch1,Notch1: metabolism,Oligonucleotide Array Sequence Analysis,Oligonucleotide Array Sequence Analysis: methods,Receptor,Signal Transduction,Software,analysis,enrichment},
mendeley-tags = {analysis,enrichment},
number = {1},
pages = {20},
pmid = {22292714},
