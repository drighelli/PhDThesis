issn = {1549-5469},
journal = {Genome research},
keywords = {Hi-C,NGS,chromosome,next generation sequencing,ngs},
mendeley-tags = {Hi-C,NGS,chromosome,next generation sequencing,ngs},
pages = {999--1011},
pmid = {24501021},
title = {{Statistical confidence estimation for Hi-C data reveals regulatory chromatin contacts.}},
