number = {1},
pages = {1--8},
pmid = {26900468},
publisher = {Cancer {\&} Metabolism},
title = {{Integration of omics: More than the sum of its parts}},
url = {http://dx.doi.org/10.1186/s40170-016-0143-y},
volume = {4},
year = {2016}
}
