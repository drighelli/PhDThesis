isbn = {2076792171},
issn = {14774054},
journal = {Briefings in Bioinformatics},
keywords = {Bioinformatics,Databases,Genomics,Omics,Proteomics,Transcriptomics},
number = {2},
pages = {286--302},
pmid = {27881428},
title = {{Genome, transcriptome and proteome: The rise of omics data and their integration in biomedical sciences}},
volume = {19},
year = {2018}
}
