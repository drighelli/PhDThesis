isbn = {0022-2844},
issn = {00222844},
journal = {Journal of Molecular Evolution},
keywords = {Genome evolution,Genomic isochores,Nucleotide substitution,Short interspersed elements,biology,gc,gccontent,sequencing},
mendeley-tags = {biology,gc,gccontent,sequencing},
number = {6},
