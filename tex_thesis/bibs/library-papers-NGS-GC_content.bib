issn = {03051048},
journal = {Nucleic Acids Research},
keywords = {biology,gc,gccontent,sequencing},
mendeley-tags = {biology,gc,gccontent,sequencing},
number = {10},
pages = {1--14},
pmid = {22323520},
title = {{Summarizing and correcting the GC content bias in high-throughput sequencing}},
volume = {40},
year = {2012}
