isbn = {9781848162990},
issn = {0919-9454},
journal = {Genome informatics. International Conference on Genome Informatics},
keywords = {Base Sequence,CpG Islands,CpG Islands: genetics,DNA,DNA: chemistry,DNA: genetics,DNA: isolation {\&} purification,Genome, Human,Humans,Models, Genetic,Nucleic Acid Conformation,Selection, Genetic,Sequence Alignment,Tandem Repeat Sequences,Tandem Repeat Sequences: genetics,biology,gc,gccontent,sequencing},
